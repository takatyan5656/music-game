{
  "name": "music-game",
  "homepage": "https://takatyan5656.github.io/music-game/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prettier": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "preview": "vite preview",
    "rm": "rm -rf docs",
    "mv": "mv dist docs",
    "git": "git add . && git commit -m 'auto deploy' && git push",
    "deploy": "npm run rm && npm run build && npm run mv && npm run git"
  },
  "devDependencies": {
    "sass": "^1.77.8",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "prettier": "^3.3.3"
  }
}
